{
  "name": "@india-boundary-corrector/leaflet",
  "version": "0.0.1",
  "description": "Leaflet integration for India boundary corrections using protomaps-leaflet",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./src/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "leaflet": ">=1.7.0",
    "protomaps-leaflet": ">=4.0.0"
  },
  "dependencies": {
    "@india-boundary-corrector/data": "^0.0.1",
    "@india-boundary-corrector/layer-configs": "^0.0.1"
  },
  "keywords": [
    "india",
    "boundary",
    "map",
    "leaflet",
    "protomaps"
  ],
  "author": "ramSeraph",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramSeraph/india_boundary_corrector.git",
    "directory": "packages/leaflet"
  },
  "bugs": {
    "url": "https://github.com/ramSeraph/india_boundary_corrector/issues"
  },
  "homepage": "https://github.com/ramSeraph/india_boundary_corrector#readme"
}
